alias ..="cd .."
[ -x /usr/local/bin/vim ] && alias vim="/usr/local/bin/vim"
[ -x /usr/local/bin/vim ] && alias vi="/usr/local/bin/vim"
[ -x /usr/local/bin/ggrep ] && alias grep="/usr/local/bin/ggrep"
alias fin="find ."


# some more ls aliases
alias ls='ls -FGh -G'
alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'
